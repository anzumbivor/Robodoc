<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.fahimahmed.robotdoc">

    <uses-permission android:name="android.permission.CALL_PHONE" />
    <uses-permission android:name="android.permission.INTERNET" />
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.DayNight.NoActionBar">
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Main2Activity" />
        <activity
            android:name=".Welcome"
            android:label="@string/title_activity_welcome"
            android:parentActivityName=".MainActivity"
            android:theme="@style/AppTheme.NoActionBar">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.fahimahmed.robotdoc.MainActivity" />
        </activity>
        <activity
            android:name=".DiseaseAnalysis"
            android:label="@string/title_activity_disease_analysis"
            android:parentActivityName=".Welcome"
            android:theme="@style/AppTheme.NoActionBar">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.fahimahmed.robotdoc.Welcome" />
        </activity>
        <activity android:name=".DisDB" />
        <activity android:name=".ShowDis" />
        <activity android:name=".SearchDocFound" />
        <activity android:name=".DocActivity" />
        <activity android:name=".Disview" />
        <activity android:name=".Doclistview" />
        <activity android:name=".newreg"></activity>
    </application>

</manifest>